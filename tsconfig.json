{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "types": ["@nuxt/types", "@nuxtjs/volar"],
    "moduleResolution": "node",
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "preserve",
    "esModuleInterop": true,
    "allowJs": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
